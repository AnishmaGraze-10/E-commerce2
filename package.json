    "build": "cd frontend && npm run build",
    "start": "cd server && npm start"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "description": "Monorepo with `client/` (React + Vite) and `server/` (Express + MongoDB). Run both for a full-stack app.",
